{
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "private": true,
  "name": "clientside",
  "version": "0.2.1",
  "description": "Bundles all client side web components and dependencies.",
  "scripts": {
    "build:release": "esbuild ./src/index.js --bundle --outdir=../dist/static --target=firefox98,chrome86,safari15 --format=iife --minify",
    "dev": "esbuild ./src/index.js --bundle --outdir=../server/static --target=firefox98,chrome86,safari15 --format=iife --watch=forever",
    "watch": "tailwindcss -i ./src/style.css -o ../server/static/style.css --minify --watch",
    "build-css:release": "tailwindcss -i ./style.css -o ./dist/static/style.css --minify"
  },
  "keywords": [],
  "author": "Timur Olur",
  "devDependencies": {
    "daisyui": "^4.12.2",
    "esbuild": "^0.21.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@carbon/charts": "^1.16.11",
    "d3": "^7.9.0",
    "d3-cloud": "^1.2.7",
    "d3-sankey": "^0.12.3",
    "htmx.org": "2.0.0",
    "idiomorph": "^0.3.0"
  }
}
