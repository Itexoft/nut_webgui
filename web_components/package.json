{
  "type": "module",
  "private": true,
  "name": "clientside",
  "version": "1.0.0",
  "description": "Bundles all client side web components and dependencies.",
  "scripts": {
    "build:release": "esbuild ./src/index.js --bundle --outdir=../dist/static --target=firefox98,chrome86,safari15 --format=iife --minify",
